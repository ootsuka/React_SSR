    "@babel/preset-stage-0": "^7.0.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "express": "^4.16.4",
    "express-http-proxy": "^1.5.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
