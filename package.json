{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all dev:build:client dev:build:server dev:start",
    "dev:build:server": "webpack --profile --json > stats_client.json --config webpack.server.js",
    "dev:build:client": "webpack --profile --json > stats_server.json --config webpack.client.js",
    "dev:start": "nodemon --watch build --exec \"node build/bundle.js\"",
    "prod": "npm-run-all --parallel prod:**",
    "prod:build:server": "webpack --config webpack.server.prod.js",
    "prod:build:client": "webpack --config webpack.client.prod.js",
    "prod:start": "node \"./build/bundle.js\"",
    "docker": "npm-run-all docker:build docker:start",
    "docker:build": "docker build --rm -t react_ssr .",
    "docker:start": "docker run --rm -it -p 3000:8080 react_ssr",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-stage-0": "^7.0.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "express": "^4.16.4",
    "express-http-proxy": "^1.5.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.3.1",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "isomorphic-style-loader": "^5.0.1",
    "jest": "^24.3.1",
    "redux-mock-store": "^1.5.3",
    "sinon": "^7.2.7",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.5"
  }
}
